{
  "name": "tweeter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Twitter clone with React Router v7 and TypeScript",
  "scripts": {
    "dev": "react-router dev",
    "dev:server": "tsx watch src/server/index.ts",
