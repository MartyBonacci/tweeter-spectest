    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "flowbite": "^2.5.1",
    "flowbite-react": "^0.10.1",
    "isbot": "^5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
